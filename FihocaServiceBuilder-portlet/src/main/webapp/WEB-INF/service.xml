<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">

<service-builder package-path="com.example.plugins">
	<namespace>FihocaServiceBuilder</namespace>
    <entity name="Employee" local-service="true" remote-service="false" table="employee_data">
        <column name="eid" type="int" primary="true" id-type="increment"></column>
        <column name="name" type="String"></column>
        <column name="address" type="String" db-name="employee_address"></column>
    </entity>
	
</service-builder>